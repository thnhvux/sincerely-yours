{
	/*                Hey there :D Having balancement issues?
	                        Join our Discord for help:
	   
	                       https://discord.gg/fvCuvYsB6C
	   
	                  Additional configs for better and modular
	                  features will be added in the future, with
	                     many configs for modpack creators!
	   
	                     ───────────────────────────────
	                         🔹  Mod Parameters 🔹
	                     ───────────────────────────────
	   
	                     ====== ⚙ Module Toggles ======
	   
	   >> Toggle systems individually for modular control <<
	   ⚠ Dynamic Targeting/Improved AI requires Stealth
	   ⚠ Critical depends on Stealth or Parry to work properly!
	*/
	"enable_stealth_system": true,
	"enable_parry_system": true,
	"enable_bash_system": true,
	/* 
	   ────────────────────────────────────────────────────────────────
	   
	              ======= ⚔ Critical Hit Settings =======
	*/
	"disable_default_critical": true,
	"enable_headshot": true,
	"enable_riposte": true,
	"enable_sneaky_attack": true,
	/* 
	   ────────────────────────────────────────────────────────────────
	   
	        ======= ⚙ Auto-generated Weapon Attributes =======
	*/
	"enable_autogenerated_critical_multiplier": true,
	"enable_autogenerated_armor_pen_attributes": true,
	"enable_autogenerated_stagger_attributes": true,
	"should_critical_scale_with_enchantments": true,
	/* 
	   ────────────────────────────────────────────────────────────────
	   
	               ======= 👁 Stealth System =======
	        Allowed difficulties -> (DEFAULT, MEDIUM, HARD)
	*/
	"stealth_difficulty": "DEFAULT",
	"armor_impact_stealthiness": true,
	"invisibility_impact_stealthiness": true,
	// True vision mobs: can detect the player even if stealth (format: 'mod_id:mob_id, ...')
	"true_vision_mobs": [
		"minecraft:enderman",
		"minecraft:warden",
		"minecraft:spider",
		"minecraft:wither",
		"minecraft:ender_dragon",
		"minecraft:phantom"
	],
	/* 
	   ────────────────────────────────────────────────────────────────
	   
	               ======= ⚔ Aggro Settings =======
	   
	   • Damage dealt/status/absorbed = threat weights
	   • Aggro leader = initial attacker bonus
	   
	   The higher the value, the more this parameter will be effective in
	   pulling a mob aggro.
	   
	   ⚠ Healing done applies to each HP restored
	*/
	"aggro_leader_multiplier": 2.0,
	"taunt_applied_weight": 5.0,
	"damage_absorbed_weight": 0.5,
	"damage_dealt_weight": 1.0,
	"healing_done_weight": 0.25,
	"effect_applied_weight": 1.0,
	/* 
	                ⚔ Threat generation values
	   
	   • Tick decay = threat fade rate
	*/
	"default_per_tick_threat_decay": 0.01,
	/* 
	   ────────────────────────────────────────────────────────────────
	   
	               ======= 👁 AI Settings =======
	*/
	"should_mobs_patrol": true,
	"should_mobs_listen_player": true,
	"should_mobs_alert_others": true,
	"mobs_listening_radius": 0.20000000298023224,
	"mobs_perception_radius": 0.20000000298023224,
	"mob_sound_whitelist": [
		"minecraft:block.wooden_door.open",
		"minecraft:block.wooden_door.close",
		"minecraft:block.iron_door.open",
		"minecraft:block.iron_door.close",
		"minecraft:block.trapdoor.open",
		"minecraft:block.trapdoor.close",
		"minecraft:block.fence_gate.open",
		"minecraft:block.fence_gate.close",
		"minecraft:block.lever.click",
		"minecraft:block.stone_button.click_on",
		"minecraft:block.stone_button.click_off",
		"minecraft:block.wooden_button.click_on",
		"minecraft:block.wooden_button.click_off",
		"minecraft:block.chest.open",
		"minecraft:block.chest.close",
		"minecraft:block.ender_chest.open",
		"minecraft:block.ender_chest.close"
	],
	/*            ======= 🛑 Patrol Blacklist =======
	   (format: 'mod_id:mob_id, ...')
	*/
	"patrol_blacklist": [
		"minecraft:enderman",
		"minecraft:warden"
	],
	/* 
	   ────────────────────────────────────────────────────────────────
	   
	                ======= 🛡️ Parry System =======
	   
	           Modes: (DEFAULT, MISSING-HEALTH, DISABLED)
	               ⚠ DISABLED disables stun and riposte
	*/
	"parry_stagger_mode": "DEFAULT",
	"base_parry_cooldown_reduction": 0,
	"should_parry_override_usable_items": false,
	"should_parry_deflect_projectiles": true,
	"should_parry_fail": true,
	/* 
	   ────────────────────────────────────────────────────────────────
	   
	                 ======= 🛡️ Bash System =======
	                      (cooldown ≥ 10 ticks)
	*/
	"base_bash_strength": 0.25,
	"bash_slowness_duration_ticks": 60,
	"should_bash_apply_slowness": true,
	"should_bash_deal_damage": true,
	/* 
	   ────────────────────────────────────────────────────────────────
	   
	               ======= ⚙ Miscellaneous =======
	*/
	"mob_fov": 90,
	"enable_custom_critical_sounds": true
}